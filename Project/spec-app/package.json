{
  "name": "spec-app",
  "version": "0.1.0",
  "license": "ISC",
  "scripts": {
    "bootstrap": "yarn global add typescript && yarn global add cwd && yarn global add tsc && yarn global add dotenv-cli && yarn global add serve && docker network create crappi-net",
    "create-network": "yarn run stop && docker network rm crappi-net & docker network create crappi-net",
    "clear-webapp": "rimraf webapp/build & rimraf webapp/node_modules",
    "build-cas": "ts-node src/scripts.ts buildOnlyCAs",
    "build-extension": "ts-node src/scripts.ts buildOnlyExtension",
    "set-keyfile": "ts-node src/scripts.ts setKeyFile",
    "build": "yarn && yarn run clear-webapp & yarn run create-network && ts-node src/scripts.ts buildAll",
    "stop": "ts-node src/scripts.ts stopAll",
    "clean": "ts-node src/scripts.ts cleanAll",
    "start": "ts-node src/index.ts",
    "start-parties": "ts-node src/scripts.ts start",
    "start-publisher": "ts-node src/publisher.ts",
    "start-auditor": "ts-node src/auditor.ts",
    "eval": "ts-node evaluation/evaluation.ts"
  },
  "dependencies": {
    "@types/lodash": "^4.14.110",
    "@types/puppeteer": "^1.5.0",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.8",
    "browserify": "^16.2.2",
    "common": "file:../common/build",
    "copyfiles": "^2.0.0",
    "lodash": "^4.17.10",
    "make-runnable": "^1.3.6",
    "proxyquire": "^2.1.0",
    "puppeteer": "^1.5.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "request": "^2.88.0",
    "rimraf": "^2.6.2",
    "serve": "^10.0.2",
    "tree-kill": "^1.2.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.0"
  }
}
